<p id="notice"><%= notice %></p>

<h1>Teams</h1>
<div class="media">
  <div class="media-body">

<table class="table table-hover">
  <thead>
    <tr>
      <th id="table_title">Zacetek iskanja</th>
      <th id="table_title">Team name</th>
      <th id="table_title">Server</th>
      <th id="table_title">Top</th>
      <th id="table_title">Jungle</th>
      <th id="table_title">Mid</th>
      <th id="table_title">Adc</th>
      <th id="table_title">Support</th>
      <th id="table_title">Iscemo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= team.Zacetek_iskanja %></td>
        <td><%= team.Team_name %></td>
        <td><%= team.Server %></td>
        <td><%= team.Top %></td>
        <td><%= team.Jungle %></td>
        <td><%= team.Mid %></td>
        <td><%= team.Adc %></td>
        <td><%= team.Support %></td>
        <td><%= team.IÜŔemo %></td>
        <td><%= link_to 'Show', team, class: 'btn btn-mini' %></td>
        <td><%= link_to 'Edit', edit_team_path(team), class: 'btn btn-mini btn-success' %></td>
        <td><%= link_to 'Destroy', team, method: :delete, class: 'btn btn-mini btn-danger', data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
   </tbody>
  </table>
 </div>
</div>

